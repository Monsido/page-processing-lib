{
    "extends": [
        "@monsido/eslint-config-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {},
    "files": [
        "src/**/*.ts",
        "src/**/*.spec.ts"
    ],
    "ignorePatterns": [
        "node_modules/",
        "dist/"
    ]
}